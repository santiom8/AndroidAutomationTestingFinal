<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Web Automation Suite">
    <parameter name="url" value="https://www.wikipedia.org/wiki/Wikipedia:Portada"/>
    <parameter name="browser" value="chrome"/>
    <parameter name="textToSearch"
               value="Microsoft"/>
   <test name="Search Test">
        <classes>
            <class name="org.example.tests.NavigationMenuBarTest"/>
        </classes>
    </test>

    <test name="Sign up Test">
        <classes>
            <class name="org.example.tests.SuccessfulSignUpTest"/>
        </classes>
    </test>
    <test name="Login Test">
        <classes>
            <class name="org.example.tests.SuccessfulLoginTest"/>
        </classes>
    </test>
    <test name="Swipe Test">
        <classes>
            <class name="org.example.tests.SwipeCardTest"/>
        </classes>
    </test>
</suite>